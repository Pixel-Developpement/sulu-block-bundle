{% set rel = (rel is defined) ? rel : 'blocks' %}

{%- block section %}
<section {% block section_attributes %}class=' max-w-none {% block section_class_attribute_value %}block {{ block.type }}{% endblock %}  {% if block['settings']['class_css'] is defined %}{{ block['settings']['class_css'] }}{% endif %}'  rel='{{ rel }}' typeof='block'{% endblock %} >
    <div class="block-container prose" {% if block['settings']['animation'] is defined %}data-aos="{{ block['settings']['animation'] }}"{% endif %} {% if block['settings']['animation_duration'] is defined %}data-aos-duration="{{ block['settings']['animation_duration']|default(1000) }}"{% endif %}>{% block section_content %}{% endblock %}</div>
</section>
{%- endblock %}
