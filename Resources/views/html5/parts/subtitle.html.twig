{% set block_property = (block_property is defined) ? block_property : 'subtitle' %}
{% set element = (element is defined) ? element : 'h2' %}

{% block before_subtitle %}{% endblock %}

{% block subtitle %}
    {% if block[block_property] is defined and block[block_property] is not empty %}
       {{ block[block_property] }}
    {% endif %}
{% endblock %}

{% block after_subtitle %}{% endblock %}
